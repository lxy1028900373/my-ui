.password-input{
  width: 100%;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.password-item-input{
  flex: 1;
  height: 96rpx;
  border:1rpx solid #ddd;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  margin-right: 4rpx;
}
.input{
  width: 0;
  height: 0;
}